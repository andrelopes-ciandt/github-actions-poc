on:
  - workflow_dispatch

name: Test paths

jobs:
  test-paths:
    runs-on: ubuntu-latest
    steps:
      - name: Checkout repo
        uses: actions/checkout@v2
        with:
          fetch-depth: 0

      - name: Log
        shell: bash
        run: |
          pwd && cd .github && pwd
          
      - name: Log 2
        shell: bash
        run: |
          pwd
